<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Report" table="report">
    <id name="reportId" type="integer" column="report_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true"/>
    <field name="dateAdded" type="datetime" column="date_added" nullable="true"/>
    <field name="createdBy" type="integer" column="created_by" nullable="true"/>
    <field name="data" type="text" column="data" nullable="true"/>
  </entity>
</doctrine-mapping>
