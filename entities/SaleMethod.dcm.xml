<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SaleMethod" table="sale_method">
    <unique-constraints>
      <unique-constraint name="sale_method_id_UNIQUE" columns="sale_method_id"/>
    </unique-constraints>
    <id name="saleMethodId" type="integer" column="sale_method_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="true"/>
  </entity>
</doctrine-mapping>
