<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * ChatChannels
 */
class ChatChannels
{
    /**
     * @var string
     */
    private $name;

    /**
     * @var boolean
     */
    private $active;

    /**
     * @var integer
     */
    private $chatChannelId;


    /**
     * Set name
     *
     * @param string $name
     * @return ChatChannels
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set active
     *
     * @param boolean $active
     * @return ChatChannels
     */
    public function setActive($active)
    {
        $this->active = $active;

        return $this;
    }

    /**
     * Get active
     *
     * @return boolean 
     */
    public function getActive()
    {
        return $this->active;
    }

    /**
     * Get chatChannelId
     *
     * @return integer 
     */
    public function getChatChannelId()
    {
        return $this->chatChannelId;
    }
}
