<?php



use Doctrine\ORM\Mapping as ORM;

/**
 * EmailFolder
 */
class EmailFolder
{
    /**
     * @var string
     */
    private $name;

    /**
     * @var integer
     */
    private $emailFolderId;


    /**
     * Set name
     *
     * @param string $name
     * @return EmailFolder
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Get emailFolderId
     *
     * @return integer 
     */
    public function getEmailFolderId()
    {
        return $this->emailFolderId;
    }
}
